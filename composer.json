{
    "name": "elao/browser-detector-bundle",
    "type": "symfony-bundle",
    "description": "Elao Browser Detector Bundle",
    "license": "MIT",
    "keywords": ["browser", "detection", "user-agent", "browscap"],
    "authors": [
        {
            "name": "Thomas Jarrand",
            "email": "thomas.jarrand@elao.com"
        },
        {
            "name": "Benjamin Lévêque",
            "email": "benjamin.leveque@elao.com"
        }
    ],
    "repositories": {
        "elao/browser-detector": {
            "url":"git@github.com:Elao/ElaoBrowserDetector.git",
            "type":"git"
        }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "~2.3",
        "elao/browser-detector": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "Elao\\Bundle\\BrowserDetectorBundle\\": "."
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
